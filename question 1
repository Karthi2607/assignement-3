#include <stdio.h>

// Function to find the cube of a number
int cube(int num) {
    return num * num * num;
}

int main() {
    int number;
    printf("Enter a number: ");
    scanf("%d", &number);
    int result = cube(number);
    printf("The cube of %d is %d\n", number, result);
    return 0;
}
